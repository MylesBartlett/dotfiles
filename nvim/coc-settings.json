{  
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": true,
  "diagnostic.virtualTextPrefix":"<< ",
  "diagnostic.checkCurrentLine": true,
  "diagnostic.enableMessage": true,
  "tslint.autoFixOnSave": true,
  "tslint.enable": true,
  "coc.preferences.formatOnType": true,
  "coc.preferences.jumpCommand": "drop",
  "coc.preferences.rootPatterns": [".git", ".vscode"],
  "coc.preferences.formatOnSaveFiletypes": ["markdown", "python", "yaml", "rust"],
  "coc-actions.hideCursor": false,
  "coc-actions.useCursorLine": true,
  "python.analysis.typeCheckingMode": "basic",
  "python.autoComplete.addBrackets": true,
  "python.jediEnabled": false,
  "python.jediMemoryLimit": -1,
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": false,
  "python.linting.flake8Args": ["--max-line-length", "100"],
  "python.formatting.provider": "black",
  "python.formatting.blackArgs": ["--line-length", "100"],
  "python.formatting.mypyEnabled": false,
  "python.sortImports.args": [
    "--line-length",
    "100"
  ],
  "coc.preferences.extensionUpdateCheck": "daily",
  "pyright.organizeimports.provider": "pyright",
  "python.linting.pydocstyleEnabled": true,
}
